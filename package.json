{
  "name": "ng-antd-workspace",
  "version": "0.0.0-UNUSED",
  "license": "MIT",
  "description": "Project used to explore Angular component design best practice",
  "main": "index.js",
  "repository": "https://github.com/trotyl/ng-antd.git",
  "author": "Trotyl <trotyl@qq.com>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prebuild-site": "ng build ng-antd",
    "build-site": "ng build --prod",
    "release": "cd dist/ng-antd && npm publish",
    "integration:cli": "cd integration/angular-cli && npm run integration",
    "pregh-build": "yarn typedoc && yarn extract-api",
    "gh-build": "ng build --prod --base-href /ng-antd/ --no-progress",
    "postgh-build": "cp dist/ng-antd-site/index.html dist/ng-antd-site/404.html && cp -r .circleci/ dist/ng-antd-site/.circleci/",
    "deploy": "gh-pages -d dist/ng-antd-site/ -t",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "extract-api": "yarn extract-api:lib && yarn extract-api:demo",
    "extract-api:lib": "tsc --lib es2015,dom tools/api-extractor/extract-lib.ts && node tools/api-extractor/extract-lib.js",
    "extract-api:demo": "tsc --lib es2015,dom tools/api-extractor/extract-demo.ts && node tools/api-extractor/extract-demo.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "^8.1.1",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "rxjs": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.1",
    "@angular-devkit/build-ng-packagr": "^0.803.4",
    "@angular/cli": "^8.3.4",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/language-service": "^8.1.1",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.7.5",
    "@types/prismjs": "^1.9.0",
    "antd": "https://github.com/ant-design/ant-design.git",
    "codecov": "^3.5.0",
    "codelyzer": "^5.0.0",
    "conventional-changelog-cli": "^2.0.23",
    "gh-pages": "^2.1.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "markdown-it": "^10.0.0",
    "ng-packagr": "^5.1.0",
    "prismjs": "^1.17.1",
    "protractor": "^5.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "ts-node": "^8.4.1",
    "tsickle": "^0.36.0",
    "tslint": "^5.15.0",
    "typedoc": "^0.15.0",
    "typescript": "~3.4.3"
  }
}
